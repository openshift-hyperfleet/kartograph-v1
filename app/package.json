{
  "name": "kartograph-visualizer",
  "version": "0.3.3",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "npx nuxt dev --dotenv .env --port 3000",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest",
    "test:run": "vitest run",
    "changelog": "node scripts/generate-changelog.js",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "release": "npm run changelog && standard-version && git add utils/changelog.ts && git commit --amend --no-edit",
    "release:minor": "npm run changelog && standard-version --release-as minor && git add utils/changelog.ts && git commit --amend --no-edit",
    "release:major": "npm run changelog && standard-version --release-as major && git add utils/changelog.ts && git commit --amend --no-edit",
    "release:patch": "npm run changelog && standard-version --release-as patch && git add utils/changelog.ts && git commit --amend --no-edit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@anthropic-ai/vertex-sdk": "^0.14.0",
    "@modelcontextprotocol/sdk": "^1.19.1",
    "@pinia/nuxt": "^0.11.2",
    "@tailwindcss/typography": "^0.5.15",
    "@types/cytoscape": "^3.21.9",
    "@vueuse/core": "^13.9.0",
    "better-auth": "^1.1.3",
    "better-sqlite3": "^11.8.1",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cytoscape": "^3.33.1",
    "cytoscape-cose-bilkent": "^4.1.0",
    "driver.js": "^1.3.6",
    "drizzle-orm": "^0.45.0",
    "google-auth-library": "^10.4.0",
    "lucide-vue-next": "^0.556.0",
    "marked": "^16.3.0",
    "nuxt": "^4.0.0",
    "pinia": "^3.0.3",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "postgres": "^3.4.7",
    "radix-vue": "^1.9.17",
    "reka-ui": "^2.5.1",
    "shiki": "^3.14.0",
    "tailwind-merge": "^3.3.1",
    "vue": "^3.5.13",
    "vue-chartjs": "^5.3.2",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^24.0.0",
    "drizzle-kit": "^0.30.1",
    "standard-version": "^9.5.0",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  }
}
